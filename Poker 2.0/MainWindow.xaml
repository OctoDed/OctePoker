<Window x:Class="Poker_2._0.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Poker_2._0"
        mc:Ignorable="d"
        Title="Poker" Height="296" Width="401" ResizeMode="NoResize" Icon="Images/icon.png">
    <Grid>
        <Label Content="Login" HorizontalAlignment="Left" Margin="84,44,0,0" VerticalAlignment="Top"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="84,103,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="LoginBox" HorizontalAlignment="Left" Height="23" Margin="84,75,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="226"/>
        <TextBox x:Name="PassBox" HorizontalAlignment="Left" Height="23" Margin="84,134,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="226" RenderTransformOrigin="0.567,-0.652"/>
        <Button x:Name="LoginButton" Click="LoginButton_Click" Content="Log in" HorizontalAlignment="Left" Margin="232,210,0,0" VerticalAlignment="Top" Width="131" Height="35" IsDefault="True"/>
        <Button x:Name="RegButton" Click="RegButton_Click" Content="Sign up" HorizontalAlignment="Left" Margin="42,210,0,0" VerticalAlignment="Top" Width="131" Height="35"/>

    </Grid>
</Window>
